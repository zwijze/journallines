<!-- Binding definition for schema model with the schema namespace as the
 default. This only effects the output format. -->
<binding value-style="attribute" track-source="true"
    package="org.jibx.schema.elements">

  <!-- All elements use the schema namespace. -->
  <namespace uri="http://www.w3.org/2001/XMLSchema" default="elements"/>
  <include path="schema-base-binding.xml"/>
  
</binding>